services:
  esourcing.apigateway:
    build:
      context: C:\Users\Rufat\Desktop\EAuctionWebApp
      dockerfile: ESourcing.ApiGateway/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ESourcing.ApiGateway
      target: base
    container_name: apigateway
    depends_on:
      esourcing.orders:
        condition: service_started
      esourcing.products:
        condition: service_started
      esourcing.sourcing:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: esourcingapigateway:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5000
      target: 80
    tty: true
    volumes:
    - C:\Users\Rufat\Desktop\EAuctionWebApp\ESourcing.ApiGateway:/app:rw
    - C:\Users\Rufat\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Rufat\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Rufat\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\Rufat\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Rufat\Desktop\EAuctionWebApp:/src:rw
  esourcing.orders:
    build:
      context: C:\Users\Rufat\Desktop\EAuctionWebApp
      dockerfile: ESourcing.Order/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ESourcing.Orders
      target: base
    container_name: esourcingorders
    depends_on:
      rabbitmq:
        condition: service_started
      redis:
        condition: service_started
      sql:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ConnectionStrings:OrderConnection: Server=sql;Database=OrderDb;User Id=sa;Password=Sqlserver2022
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      EventBus:HostName: rabbitmq
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: esourcingorders:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8003
      target: 80
    tty: true
    volumes:
    - C:\Users\Rufat\Desktop\EAuctionWebApp\ESourcing.Order:/app:rw
    - C:\Users\Rufat\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Rufat\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Rufat\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\Rufat\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Rufat\Desktop\EAuctionWebApp:/src:rw
  esourcing.products:
    build:
      context: C:\Users\Rufat\Desktop\EAuctionWebApp
      dockerfile: ESourcing.Products/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ESourcing.Products
      target: base
    container_name: esourcingproducts
    depends_on:
      mongo:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
      ProductDatabaseSettings:ConnectionStrings: mongodb://mongo:27017
    image: esourcingproducts:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/ESourcing.Products.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8002
      target: 80
    tty: true
    volumes:
    - C:\Users\Rufat\Desktop\EAuctionWebApp\ESourcing.Products:/app:rw
    - C:\Users\Rufat\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Rufat\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Rufat\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\Rufat\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Rufat\Desktop\EAuctionWebApp:/src:rw
  esourcing.sourcing:
    build:
      context: C:\Users\Rufat\Desktop\EAuctionWebApp
      dockerfile: ESourcing.Sourcing/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ESourcing.Sourcing
      target: base
    container_name: esourcingsourcing
    depends_on:
      mongo:
        condition: service_started
      rabbitmq:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      EventBus:HostName: rabbitmq
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
      SourcingDatabaseSettings:ConnectionString: mongodb://mongo:27017
    image: esourcingsourcing:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/ESourcing.Sourcing.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8001
      target: 80
    tty: true
    volumes:
    - C:\Users\Rufat\Desktop\EAuctionWebApp\ESourcing.Sourcing:/app:rw
    - C:\Users\Rufat\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Rufat\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Rufat\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\Rufat\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Rufat\Desktop\EAuctionWebApp:/src:rw
  mongo:
    container_name: mongo
    image: mongo
    ports:
    - published: 27017
      target: 27017
    restart: always
    volumes:
    - /site:/data/db:rw
  rabbitmq:
    container_name: rabbitmq
    image: rabbitmq:3-management
    ports:
    - published: 5672
      target: 5672
    - published: 15672
      target: 15672
    restart: always
  redis:
    container_name: redis
    image: redis
    ports:
    - published: 6333
      target: 6379
    restart: always
  sql:
    container_name: sql
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Sqlserver2022
    image: mcr.microsoft.com/mssql/server
    ports:
    - published: 1433
      target: 1433
    restart: always
version: '3.4'